import "common.cgl"

main = shape{
    ss: []
    num = 70
    for i in 0:num-1 do(
        ss[i] = square{
            scale = {x: 30, y: 30}
            pos = {x: 45*i, y:200*Sin(170*i)}
            angle = i*35
        }
        var(ss[i].pos, ss[i].angle)
    )
    for i in 0:num/2-1 do(
        sat( contact(ss[i*2].bottomRight(), ss[i*2+1].bottomLeft()) )
    )
}

