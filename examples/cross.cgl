shape = {
    pos: {x:0, y:0}
    scale: {x:1, y:1}
    angle: 0
}

stick = shape{
    scale = {x:1, y:3}
    vertex: [
        {x: -1, y: -1}, {x: +1, y: -1}
        {x: +1, y: +1}, {x: -1, y: +1}
    ]
}

plus = shape{
    scale = {x:50, y:50}
    a: stick{}
    b: stick{angle = 90}
}

cross = shape{
    pos = {x:256, y:256}
    a: plus{angle = 45}
}

