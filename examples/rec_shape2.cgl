shapeList = (f, depth, template -> 
    if depth == 0
    then Shape{
        a: template{}
    }
    else Shape{
        a: template{}
        b: f(f, depth-1, template){
            pos.y = -1
            angle = 30
            scale = X2(0.9)
        }
    }
)

tree = Shape{
    depth = 50
    a: shapeList(shapeList, depth, Shape{a:Diff(Triangle{}, Buffer(Triangle{}, -0.1))}){scale = X2(100)}
    a.b.b.b.b.b = 0
}
